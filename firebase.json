{
  "hosting": {
    "site" : "drnull",
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "run": {
        "serviceId": "flask"   ,
        "region": "me-central1",   
        "pinTag": true             
      }
    } ]
  }
}